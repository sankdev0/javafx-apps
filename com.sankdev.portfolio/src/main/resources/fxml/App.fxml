<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml"
			styleClass="border-pane" fx:controller="com.sankdev.portfolio.MainFXMLController"
			>
	<top>
		<HBox styleClass="h-box" xmlns:fx="http://javafx.com/fxml">
			<children>
				<Button fx:id="editButton" text="%editButton">
				</Button>
				<Button fx:id="deleteButton" text="%deleteButton">
				</Button>
				<Button fx:id="addButton" text="%addButton">
				</Button>
				<Button fx:id="saveButton" text="%saveButton">
				</Button>
				<Button fx:id="loadButton" text="%loadButton">
				</Button>
			</children>
		</HBox>
	</top>

</BorderPane>